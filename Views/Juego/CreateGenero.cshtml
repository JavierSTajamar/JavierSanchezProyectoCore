@using JavierSanchezProyectoCore.Models
@model Genero

<form class="form-group" asp-action="CreateGenero">

    <label class="form-label" asp-for="Nombre">Genero</label>
    <input class="form-control" asp-for="Nombre" />
    <span class="text-red" asp-validation-for="Nombre"></span>
    <br />
    <button class="btn btn-primary" type="submit">Añadir Genero</button>
</form>
